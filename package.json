{
	"name": "@audira/carbon-react-native",
	"version": "0.0.1",
	"private": true,
	"type": "module",
	"scripts": {
		"builder-bob": "node ./libs/builder-bob/scripts/build.js",
		"code-check": "npm run type-check && npm run lint",
		"lint": "eslint .",
		"setup-package-registry": "npm run ts-node-esm ./scripts/setup-package-registry",
		"ts-node-esm": "node --loader ts-node/esm -r tsconfig-paths/register",
		"tsx": "tsx",
		"type-check": "tsc --noEmit && tsc --project docs/tsconfig.json && tsc --project example/tsconfig.json && tsc --project package/tsconfig.json"
	},
	"devDependencies": {
		"@docusaurus/eslint-plugin": "^3.7.0",
		"@eslint/compat": "^1.2.7",
		"@eslint/js": "^9.22.0",
		"@react-native/eslint-config": "0.78.0",
		"@stylistic/eslint-plugin": "^4.2.0",
		"@types/node": "^22.13.10",
		"@types/react": "^19.0.10",
		"eslint": "^9.22.0",
		"eslint-import-resolver-typescript": "^3.8.3",
		"eslint-plugin-import-x": "^4.6.1",
		"eslint-plugin-react": "^7.37.4",
		"eslint-plugin-react-native": "^5.0.0",
		"globals": "^16.0.0",
		"ts-node": "^10.9.2",
		"tsconfig-paths": "^4.2.0",
		"tsx": "^4.19.2",
		"typescript": "5.8.2",
		"typescript-eslint": "^8.26.0"
	}
}
