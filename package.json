{
	"name": "@raka/carbon-react-native",
	"version": "0.0.1-prealpha.1",
	"private": true,
	"type": "module",
	"scripts": {
		"code-check": "npm run type-check && npm run lint",
		"create-package-symlink": "npm run ts-node-esm ./scripts/create-package-symlink/index.ts -- --destination",
		"lint": "eslint . --fix",
		"release": "echo \"TODO: It will be determined later\"",
		"ts-node-esm": "node --experimental-specifier-resolution=node --loader ts-node/esm -r tsconfig-paths/register",
		"type-check": "tsc --noEmit"
	},
	"devDependencies": {
		"@docusaurus/eslint-plugin": "^3.7.0",
		"@eslint/compat": "^1.2.5",
		"@react-native/eslint-config": "0.76.6",
		"@stylistic/eslint-plugin": "^2.12.1",
		"@types/node": "^22.10.5",
		"@types/react": "^18.2.6",
		"eslint": "^9.17.0",
		"eslint-plugin-react-native": "^5.0.0",
		"globals": "^15.14.0",
		"ts-node": "^10.9.2",
		"tsconfig-paths": "^4.2.0",
		"typescript": "5.7.2",
		"typescript-eslint": "^8.19.1"
	}
}
