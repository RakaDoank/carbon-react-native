{
	"name": "@rakadoank/carbon-react-native",
	"version": "0.0.1-prealpha.1",
	"scripts": {
		"code-check": "npx tsc && npm run lint --fix",
		"lint": "eslint .",
		"prepare": "bob build"
	},
	"peerDependencies": {
		"@carbon/icon-helpers": "*",
		"eslint": ">=8",
		"react": "*",
		"react-native": ">=0.73",
		"react-native-reanimated": ">=3",
		"react-native-svg": ">=15"
	},
	"devDependencies": {
		"@react-native/eslint-config": "0.76.2",
		"@react-native/metro-config": "0.76.2",
		"@stylistic/eslint-plugin": "2.10.1",
		"@types/react": "^18.2.6",
		"@typescript-eslint/eslint-plugin": "^7.1.1",
		"@typescript-eslint/parser": "^7.1.1",
		"eslint": "8.57.1",
		"eslint-import-resolver-typescript": "^3.6.3",
		"eslint-plugin-import": "^2.31.0",
		"react-native-builder-bob": "^0.33.1",
		"typescript": "5.6.3"
	},
	"source": "./src/index.tsx",
	"main": "./lib/commonjs/index.js",
	"module": "./lib/module/index.js",
	"exports": {
		".": {
			"import": {
				"types": "./lib/typescript/module/src/index.d.ts",
				"react-native": "./lib/modules/index.native.js",
				"default": "./lib/module/index.js"
			},
			"require": {
				"types": "./lib/typescript/commonjs/src/index.d.ts",
				"react-native": "./lib/commonjs/index.native.js",
				"default": "./lib/commonjs/index.js"
			}
		}
	},
	"files": [
		"src",
		"lib",
		"configs",
		"!**/__tests__",
		"!**/__fixtures__",
		"!**/__mocks__",
		"!**/.*"
	],
	"react-native-builder-bob": {
		"source": "src",
		"output": "lib",
		"targets": [
			[
				"commonjs",
				{
					"esm": true
				}
			],
			[
				"module",
				{
					"esm": true
				}
			],
			[
				"typescript",
				{
					"esm": true
				}
			]
		]
	}
}