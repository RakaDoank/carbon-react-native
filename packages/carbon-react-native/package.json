{
	"name": "@audira/carbon-react-native",
	"version": "0.0.1-alpha.16",
	"license": "MIT",
	"homepage": "https://github.com/RakaDoank/carbon-react-native",
	"repository": "https://github.com/RakaDoank/carbon-react-native",
	"description": "Build React Native apps with component and shared patterns using Carbon",
	"type": "module",
	"peerDependencies": {
		"@audira/carbon-react-native-elements": "workspace:*",
		"@carbon/icons": ">=11",
		"@carbon/icon-helpers": ">=10",
		"react": ">=17",
		"react-native": ">=0.72",
		"react-native-reanimated": ">=3",
		"react-native-svg": ">=15"
	},
	"devDependencies": {
		"@react-native/typescript-config": "0.79.2",
		"typescript": "5.8.2"
	},
	"main": "./lib/commonjs/index.js",
	"module": "./lib/module/index.js",
	"types": "./lib/typescript/commonjs/src/index.d.ts",
	"exports": {
		".": {
			"import": {
				"types": "./lib/typescript/module/src/index.d.ts",
				"default": "./lib/module/index.js"
			},
			"require": {
				"types": "./lib/typescript/commonjs/src/index.d.ts",
				"default": "./lib/commonjs/index.js"
			}
		}
	},
	"files": [
		"lib",
		"assets",
		"configs",
		"!**/__tests__",
		"!**/__fixtures__",
		"!**/__mocks__",
		"!**/.*"
	],
	"keywords": [
		"carbon-react-native",
		"audira-carbon-react-native",
		"carbon-components",
		"react-native",
		"android",
		"ios",
		"windows",
		"macos",
		"web"
	]
}